{% extends 'base.html.twig' %}

{% block title %}Feu{% endblock %}

{% block body %}
    <h1>Feu</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feu.id }}</td>
            </tr>
            <tr>
                <th>Datos</th>
                <td>{{ feu.datos }}</td>
            </tr>
            <tr>
                <th>Firma</th>
                <td>{{ feu.firma }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
